{
    "compilerOptions": {
      "target": "es5",
      "module": "commonjs",
      "lib": ["dom"],
      "outDir": "./build",
      "strict": true,
      "noImplicitAny": false,
      "strictPropertyInitialization": false,
      "baseUrl": "./",
      "esModuleInterop": true,
      "experimentalDecorators": true,
      "emitDecoratorMetadata": true
    },
    "include": [
      "./**/*",
    ],
    "exclude": [
      "node_modules"
    ]
  }